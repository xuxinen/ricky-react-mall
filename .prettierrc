{
	"editor.formatOnSave": true,
  "indent_size": 2,
	"semi": true,
	"semicolons": true,
	"printWidth": 168,
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"rangeStart": 0,
	"rangeEnd": 500,
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "always",
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": false,
	"endOfLine": "lf"
}
